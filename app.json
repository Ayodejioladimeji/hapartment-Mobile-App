{
  "expo": {
    "name": "Hapartment",
    "slug": "hapartment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#449342"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#449342"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.layobright.hapartment"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow Hapartment to access your camera"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1a03b837-9f82-4b4d-b980-a052e3d3f447"
      }
    }
  }
}
